{
    "publish": {
        "navimow_running_status": {
            "command": "what is Navimow i105 doing",
            "regex": ".*105 (.*)\\.",
            "result_map": {
                "is running": "Run",
                "isn't docked": "Run",
                "is paused": "Pause",
                "isn't running": "Dock",
                "is docked": "Dock"
            }
        },
        "navimow_battery_status": {
            "command": "what is Navimow i105 battery level",
            "regex": "([0-9]+) *percent"
        }
    },
    "subscribe": {
        "navimow_running": {
            "Run": "start Navimow i105",
            "Pause": "pause Navimow i105",
            "Dock": "dock Navimow i105"
        }
    }
}
